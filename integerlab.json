{
  "targetFiles": [
    "alu.c",
    "basetwo.c"
  ],
  "commentDelimiters": {
    "inlineComment": "//",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "codeBlockDelimiters": [
    "{",
    "}"
  ],
  "codeBlockKeywords": {
    "loops": [
      "do",
      "while",
      "for"
    ],
    "benignBlockTerminations": {
      "break": "switch"
    }
  },
  "operators": {
    "all": [
      "++",
      "--",
      ".",
      "->",
      "!",
      "~",
      "*",
      "/",
      "%",
      "+",
      "-",
      "<<",
      ">>",
      "<",
      "<=",
      ">",
      ">=",
      "==",
      "!=",
      "&",
      "^",
      "|",
      "&&",
      "||",
      "?",
      "=",
      "+=",
      "-=",
      "*=",
      "/=",
      "%=",
      "<<=",
      ">>=",
      "&=",
      "^=",
      "|=",
      ","
    ],
    "disallowed": [
      "+",
      "-",
      "*",
      "/",
      "%",
      "+=",
      "-=",
      "*=",
      "/=",
      "%=",
      "++",
      "--"
    ]
  },
  "disallowedCalls": [],
  "disallowedKeywords": [
    "goto"
  ],
  "limitedLoopTerminations": {
    "0": [
      "break",
      "continue",
      "return"
    ]
  },
  "limitedCharacters": {
    "1": [
      "#"
    ]
  }
}
